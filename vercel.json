{
  "buildCommand": null,
  "functions": {
    "api/get-news.js": {
      "maxDuration": 10
    },
    "api/get-timetable.js": {
      "maxDuration": 10
    },
    "api/get-wheel-students.js": {
      "maxDuration": 10
    },
    "api/get-teaching-plan.js": {
      "maxDuration": 10
    },
    "api/get-targets.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}